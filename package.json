{
  "name": "dcv-tool",
  "version": "0.0.6",
  "description": "工具类",
  "author": "jw",
  "scripts": {
    "dev": "webpack-dev-server",
    "develop": "webpack --mode development",
    "merge": "webpack --mode production",
    "lint": "./node_modules/.bin/tslint src/**/*.ts",
    "commitLint": "./node_modules/.bin/commitlint -E HUSKY_GIT_PARAMS",
    "fix": "./node_modules/.bin/tslint --fix src/**/*.ts",
    "unit": "./node_modules/.bin/jest --coverage",
    "compile": "tsc --target es2015",
    "build": "npm run merge && npm run compile"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "npm run commitLint"
    }
  },
  "main": "dist/browser/index.js",
  "typings": "ts/browser/index.d.ts",
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-angular": "^7.1.2",
    "@commitlint/is-ignored": "^7.2.1",
    "@commitlint/lint": "^7.2.1",
    "@types/jest": "^24.0.12",
    "chai": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "tslint": "^5.16.0",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "license": "ISC"
}
